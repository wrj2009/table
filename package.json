{
  "name": "table",
  "version": "1.8.1",
  "description": "显示包含日期、应到人数、实到人数和请假人姓名的表格。",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --ia32"
  },
  "author": "Scratcher-wrj",
  "license": "MIT",
  "build": {
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "学生信息表",
      "deleteAppDataOnUninstall": true,
      "uninstallDisplayName": "学生信息表",
      "installerIcon": "./icon.ico",
      "uninstallerIcon": "./icon.ico",
      "installerHeaderIcon": "./icon.ico"
    }
  },
  "config": {},
  "devDependencies": {
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "os": "^0.1.2"
  }
}
